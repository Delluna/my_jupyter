def simple_search(lst, e):
    for index, item in enumerate(lst):
        if item == e:
            return index
    return None





def binary_search(lst, e):
    low = 0
    high = len(lst) - 1
    while low <= high:
        cur = (low + high) // 2
        if lst[cur] == e:
            return cur
        elif lst[cur] > e:
            high = cur - 1
        elif lst[cur] < e:
            low = cur + 1
    return None



























def biggest(lst):
    value = lst[0]
    index = 0
    for i in range(len(lst)):
        if value < lst[i]:
            value = lst[i]
            index = i
    lst.pop(index)
    return lst, value

def selection_sort(lst):
    sorted_lst = []
    while lst != []:
        list, e = biggest(lst)
        sorted_lst.append(e)
    return sorted_lst





































def quick_sort(lst):
    if len(lst) < 2:
        return lst
    else:
        pivot = lst[0]
        less = []
        greater = []
        for i in range(1, len(lst)):
            if lst[i] <= pivot:
                less.append(lst[i])
            else:
                greater.append(lst[i])
        return quick_sort(less) + [pivot] + quick_sort(greater)





def merge(lst1, lst2):
    if len(lst1) == 0:
        return [lst2[0]] 
    elif len(lst2) == 0:
        return [lst1[0]] 
    else:
        lst = []
        i = 0
        j = 0
        while i < len(lst1) and j < len(lst2):
            if lst1[i] <= lst2[j]:
                lst.append(lst1[i])
                i += 1
            else:
                lst.append(lst2[j])
                j += 1
        if i == len(lst1):
            while j < len(lst2):
                lst.append(lst2[j])
                j += 1
        if j == len(lst2):
            while i < len(lst1):
                lst.append(lst1[i])
                i += 1
        return lst     
        
def merge_sort(lst):
    lst1, lst2 = lst[0:len(lst)//2], lst[len(lst)//2:]
    if len(lst1) == 0 or len(lst2) == 0:
        return [lst2[0]] if len(lst1) == 0 else [lst1[0]]
    elif len(lst1) == 1 and len(lst2) == 1:
        return [lst1[0], lst2[0]] if lst1[0] <= lst2[0] else [lst2[0], lst1[0]]
    else:
        return merge(merge_sort(lst1), merge_sort(lst2))

